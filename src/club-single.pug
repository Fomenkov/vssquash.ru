					.club__images-grid-picture
						img.img.js-gallery-picture(src='images/club-arvy.jpg' alt='Сквош-клуб')
					.club__images-grid-picture
						img.img.js-gallery-picture(src='images/club-citysquash.jpg' alt='Сквош-клуб')
					.club__images-grid-picture
						img.img.js-gallery-picture(src='images/club-golf.jpg' alt='Сквош-клуб')
					.club__images-grid-picture
						img.img.js-gallery-picture(src='images/club-multisport.jpg' alt='Сквош-клуб')
					.club__images-grid-picture
						img.img.js-gallery-picture(src='images/club-arvy.jpg' alt='Сквош-клуб')
					.club__images-grid-picture
						img.img.js-gallery-picture(src='images/club-citysquash.jpg' alt='Сквош-клуб')
